@import '../../mixinspack/mixins.styl'

.footer-cont
    sizer(100vw,25vh)
    position:absolute;
    bottom:0
    display: flex;
    justify-content: center
    border-top: 2px solid black  
    border-bottom: 2px solid black  

.content
    sizer(80%,100%)
    margin-top:2%
    display:flex
    flex-direction: row
    justify-content: space-around

.content h2
    color:#343434
    font-weight: 300
    line-height: 20px

#logo
    sizer(50px,50px)